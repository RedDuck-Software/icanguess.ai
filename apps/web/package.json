{
  "name": "template-vite-tailwind-shadcn",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "yarn format:prettier && yarn format:es",
    "format:prettier": "prettier --write \"src/**/*.{ts,tsx,d.ts}\"",
    "format:es": "eslint --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0 --fix .",
    "lint": "yarn lint:prettier && yarn lint:es",
    "lint:prettier": "prettier --check \"src/**/*.{ts,tsx,d.ts}\"",
    "lint:es": "eslint --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0 .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@tanstack/react-query": "^5.45.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "i": "^0.3.7",
    "lucide-react": "^0.395.0",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.22.3",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
